SMS_USE_DHIRAAGU=true

# Fallback: HTTP gateway (when SMS_USE_DHIRAAGU=false)
SMS_GATEWAY_URL=https://akuru.edu.mv/api/v2
SMS_GATEWAY_API_KEY=
SMS_GATEWAY_ENABLED=false

# BML Connect Payment Gateway (Redirect method; webhook is PRIMARY for confirmation)
BML_BASE_URL=https://api.uat.merchants.bankofmaldives.com.mv/public
BML_APP_ID=
BML_API_KEY=
BML_MERCHANT_ID=
BML_WEBHOOK_SECRET=
BML_WEBHOOK_URL=
BML_RETURN_URL=
BML_PROVIDER=
BML_DEFAULT_CURRENCY=MVR
BML_ENVIRONMENT=sandbox
BML_WEBHOOK_SIGNATURE_HEADER=X-BML-Signature
BML_WEBHOOK_IP_ALLOWLIST=
