# Analytics (optional)
GA_MEASUREMENT_ID=

# Google Maps embed (optional - get from Google Maps > Share > Embed for exact location)
# GOOGLE_MAPS_EMBED_URL=

# CDN (optional - set ASSET_URL for Laravel to use CDN for assets)
ASSET_URL=
CDN_ENABLED=false

# SMS - Dhiraagu config (from akurusms)
DHIRAAGU_SMS_USERNAME=
DHIRAAGU_SMS_PASSWORD=
